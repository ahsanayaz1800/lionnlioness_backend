{
    "version": 2,
    "builds": [
      {
        "src": "routes/server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "routes/server.js",
        "methods":["GET","POST","PUT", "DELETE", "PATCH", "OPTIONS"],
        "headers":{
            "Access-Control-Allow-Origin":"*"
        }
      },
      {
        "src": "/(.*)",
        "dest": "routes/server.js",
        "methods":["GET","POST","PUT", "DELETE", "PATCH", "OPTIONS"],
        "headers":{
            "Access-Control-Allow-Origin":"*"
        }
      }
    ]
  }
